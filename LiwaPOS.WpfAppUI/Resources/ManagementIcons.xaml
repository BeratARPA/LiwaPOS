<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <DrawingImage x:Key="ManagementSettingsIcon">
        <DrawingImage.Drawing>
            <GeometryDrawing  Brush="{StaticResource Window.Static.Foreground}" Geometry="{StaticResource Settings}"/>
        </DrawingImage.Drawing>
    </DrawingImage>
    
    <DrawingImage x:Key="ManagementProductsIcon">
        <DrawingImage.Drawing>
            <GeometryDrawing Brush="{StaticResource Window.Static.Foreground}" Geometry="{StaticResource Products}"/>
        </DrawingImage.Drawing>
    </DrawingImage>
    
    <DrawingImage x:Key="ManagementTicketsIcon">
        <DrawingImage.Drawing>
            <GeometryDrawing Brush="{StaticResource Window.Static.Foreground}" Geometry="{StaticResource Tickets}"/>
        </DrawingImage.Drawing>
    </DrawingImage>

    <DrawingImage x:Key="ManagementPrintingIcon">
        <DrawingImage.Drawing>
            <GeometryDrawing Brush="{StaticResource Window.Static.Foreground}" Geometry="{StaticResource Printing}"/>
        </DrawingImage.Drawing>
    </DrawingImage>
    
    <DrawingImage x:Key="ManagementReportsIcon">
        <DrawingImage.Drawing>
            <GeometryDrawing Brush="{StaticResource Window.Static.Foreground}" Geometry="{StaticResource Reports}"/>
        </DrawingImage.Drawing>
    </DrawingImage>
    
    <DrawingImage x:Key="ManagementAutomationsIcon">
        <DrawingImage.Drawing>
            <GeometryDrawing Brush="{StaticResource Window.Static.Foreground}" Geometry="{StaticResource Automation}"/>
        </DrawingImage.Drawing>
    </DrawingImage>
    
    <DrawingImage x:Key="ManagementUsersIcon">
        <DrawingImage.Drawing>
            <GeometryDrawing Brush="{StaticResource Window.Static.Foreground}" Geometry="{StaticResource Users}"/>
        </DrawingImage.Drawing>
    </DrawingImage>
    
</ResourceDictionary>